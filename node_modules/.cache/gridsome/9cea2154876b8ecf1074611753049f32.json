div class=\"col-md-10 col-lg-8 col-xl-7\">\r\n            <div class=\"site-heading\">\r\n              <h1>{{ general.title }}</h1>\r\n              <span class=\"subheading\">{{ general.subtitle }}</span>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </header>\r\n    <!-- Main Content-->\r\n    <div class=\"container px-4 px-lg-5\">\r\n      <div class=\"row gx-4 gx-lg-5 justify-content-center\">\r\n        <div class=\"col-md-10 col-lg-8 col-xl-7\">\r\n          <!-- Post preview-->\r\n          <div class=\"post-preview\" v-for=\"post in $page.posts.edges\" :key=\"post.node.id\">\r\n            <g-link :to=\"'/post/'+ post.node.id\">\r\n              <h2 class=\"post-title\">\r\n                {{ post.node.title }}\r\n              </h2>\r\n              <!-- <h3 class=\"post-subtitle\">\r\n                Problems look mighty small from 150 miles up\r\n              </h3> -->\r\n            </g-link>\r\n            <p class=\"post-meta\">\r\n              Posted by\r\n              <a href=\"#!\">{{ post.node.author.name }}</a>\r\n              on {{ post.node.created_at}}\r\n            </p>\r\n            <span v-for=\"tag in post.node.tags\" :key=\"tag.id\">\r\n                <g-link :to=\"`/tag/${tag.id}`\">{{ tag.tagName }}</g-link>\r\n                &nbsp;&nbsp;\r\n            </span>\r\n            <!-- Divider-->\r\n            <hr class=\"my-4\" />\r\n\r\n          </div>\r\n          \r\n          <!-- Pager-->\r\n          <Pager :info=\"$page.posts.pageInfo\"/>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </Layout>\r\n</template>\r\n\r\n<page-query>\r\nquery ($page: Int){\r\n  posts: allStrapiPosts(perPage:2, page:$page) @paginate {\r\n     pageInfo {\r\n      totalPages\r\n      currentPage\r\n    }\r\n    edges {\r\n      node {\r\n        id\r\n        title\r\n        content\r\n        tags{\r\n          id\r\n          tagName\r\n        }\r\n        author{\r\n          name\r\n          picture{\r\n            url\r\n          }\r\n        }\r\n        created_at\r\n      }\r\n    }\r\n  }\r\n  general:allStrapiGeneral {\r\n    edges{\r\n      node {\r\n        title\r\n        subtitle\r\n        cover{\r\n          url\r\n        }\r\n      }\r\n    }\r\n  }\r\n}\r\n</page-query>\r\n\r\n<script>\r\nimport { Pager } from 'gridsome'\r\nexport default {\r\n  components: {\r\n    Pager\r\n  },\r\n  metaInfo: {\r\n    title: \"Hello, world!\",\r\n  },\r\n  computed: {\r\n      general() {\r\n          return this.$page.general.edges[0].node\r\n      }\r\n  },\r\n};\r\n</script>\r\n\r\n<style></style>\r\n"]}]}