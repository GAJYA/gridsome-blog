bel>\r\n                </div>\r\n                <div class=\"form-floating\">\r\n                  <input\r\n                    v-model=\"forms.phone\"\r\n                    class=\"form-control\"\r\n                    id=\"inputPhone\"\r\n                    type=\"tel\"\r\n                    placeholder=\"Enter your phone number...\"\r\n                  />\r\n                  <label for=\"inputPhone\">Phone Number</label>\r\n                </div>\r\n                <div class=\"form-floating\">\r\n                  <textarea\r\n                    v-model=\"forms.message\"\r\n                    class=\"form-control\"\r\n                    id=\"inputMessage\"\r\n                    placeholder=\"Enter your message here...\"\r\n                    style=\"height: 12rem\"\r\n                  ></textarea>\r\n                  <label for=\"inputMessage\">Message</label>\r\n                </div>\r\n                <br />\r\n                <button\r\n                  class=\"btn btn-primary text-uppercase\"\r\n                  type=\"button\"\r\n                  @click=\"onSubmit\"\r\n                >\r\n                  Send\r\n                </button>\r\n              </form>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </main>\r\n  </Layout>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nexport default {\r\n  name: \"ContactPage\",\r\n  data() {\r\n    return {\r\n      forms: {\r\n        name: \"\",\r\n        email: \"\",\r\n        phone: \"\",\r\n        message: \"\",\r\n      },\r\n    };\r\n  },\r\n  methods: {\r\n    async onSubmit() {\r\n      try {\r\n        await axios.post(this.GRIDSOME_API_URL+\"/contacts\", this.forms);\r\n        window.alert(\"恭喜提交成功\");\r\n        for (const key in this.forms) {\r\n            this.forms[key] = ''\r\n        }\r\n      } catch (error) {\r\n        window.alert(\"请求出错，请稍后重试\");\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style></